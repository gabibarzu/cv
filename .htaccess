RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule ^.*[-](\d+)/$ themeso/index\.php?id=$1&%{QUERY_STRING} [L]
RewriteRule ^.*g/(.*)/$ themeso/index\.php?cat=$1&%{QUERY_STRING} [L]
RewriteRule ^([^\.]+)$ $1.php [NC,L]

